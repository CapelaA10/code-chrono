{
  "productName": "Code Chrono",
  "version": "0.2.0",
  "identifier": "com.codechrono.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "Code Chrono",
        "width": 1000,
        "height": 720,
        "minWidth": 800,
        "minHeight": 600,
        "label": "main",
        "decorations": true,
        "resizable": true
      }
    ],
    "security": {
      "capabilities": [
        "default"
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico",
      "icons/icon.png"
    ]
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/CapelaA10/code-chrono/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IERGQkZBMTNBMTFBNzhBNTYKUldSV2lxY1JPcUcvMy9RVEZvMUlNV0RyR1R6U0tPWTRnVzNBejA5Q0JaM1RVWGhhM1c2WUxUVS8K",
      "windows": {
        "installMode": "passive"
      }
    }
  }
}